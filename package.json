{
  "name": "rsc_movies_rwsdk",
  "version": "0.1.0",
  "description": "RedwoodSDK Movies RSC",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "clean": "rm -rf node_modules/.vite",
    "release": "npm run clean && npm run build && wrangler deploy",
    "db:create": "wrangler d1 create rsc_movies_rwsdk",
    "db:seed": "wrangler d1 execute rsc_movies_rwsdk --local --file migrations/0001_init.sql",
    "db:seed:prod": "wrangler d1 execute rsc_movies_rwsdk --file migrations/0001_init.sql"
  },
  "dependencies": {
    "@ryanflorence/batch-loader": "^0.0.1",
    "rwsdk": "0.0.83"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "typescript": "^5.8.3",
    "vite": "^6.2.6",
    "wrangler": "^4.14.1"
  }
}
